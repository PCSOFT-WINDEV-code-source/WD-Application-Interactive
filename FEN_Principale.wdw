     name : RafraichitTempératures
     internal_properties : CAAAAAgAAAAJoi9uSeE5XMf/mARTAGf+YsOBDKefNqZyPr8xLkf4htViZ5Xg6So96Jyj6XOrRgDVhMqtlk19JVNHXojJm4Ik4vvifgQILAbvE4Jp9zpDEDw8KsWb/Bv4xY5LEDShK1nY2B5aYj98SCTnMinQIA/wi+m91I7938U677kvBp5Na5ii9DWm3Q==
     procedure_id : 1609056497550678771
     type_code : 14
     code : |1+
      // Résumé : Rafraîchit les températures
      // 
      // Procédure automatique :
      // La procédure est exécutée automatiquement, après le code d'initialisation de la fenêtre, avec un différé de 1 seconde
      // Elle sera répétée en boucle, en attendant 1 seconde entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      
      PROCEDURE RafraichitTempératures()
      
      
      pour i = 1 _A_ 4 
      	// Initialise le générateur de nombres aléatoires
      	InitHasard()
      	
      	xNombreAléatoire est un numérique
      	xNombreAléatoire = Hasard(190, 260) / 10
      	
      	// Arrondi
      	xNombreAléatoire = Arrondi(xNombreAléatoire, 1)
      	
      	{"CFI_Edition.LIB_Température_" + i,indChamp} = xNombreAléatoire
      FIN
     type : 458752
   -
     name : RécupèreEtatLumières
